{
    "name": "potherca/katwizy",
    "description": "🌟 A cleaner Symfony install for light-weight projects. 🏁🚗💨",
    "keywords": ["symfony", "install", "installer", "lightweight", "micro", "potherca"],
    "homepage": "https://github.com/Potherca/Katwizy/",
    "type": "library",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Ben Peachey",
            "email": "potherca@gmail.com",
            "homepage": "https://pother.ca"
        }
    ],
    "support": {
        "issues": "https://github.com/Potherca/Katwizy/issues",
        "source": "https://github.com/Potherca/Katwizy/"
    },
    "minimum-stability": "stable",
    "require": {
        "symfony/framework-standard-edition": "^3.0",
        "sensio/generator-bundle": "^3.0"
     },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Potherca\\Katwizy\\": "src/"
        }
    },
    "bin": ["bin/console", "bin/flotsam", "bin/symfony_requirements"]
}
